@model Zarzadzanie_Projektami.Models.Profil

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Profil</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Login)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Login)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Imie)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Imie)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Nazwisko)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nazwisko)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DataUtworzeniaKonta)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DataUtworzeniaKonta)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Projekty)
        </dt>
        <dd>
            <table class="table">
                <tr>
                    <th>Nazwa projektu</th>
                    <th>Opis</th>
                </tr>
                @foreach (var item in Model.Projekty)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Nazwa)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Opis)
                        </td>
                    </tr>
                }
            </table>
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
